    CREATE TABLE `db_ipp`.`table_estudante` 
        ( `id_estudante` INT(11) NOT NULL AUTO_INCREMENT , `nr_estudante` VARCHAR(15) NOT NULL , `fullname` VARCHAR(50) NOT NULL , `datanascimento` VARCHAR(10) NOT NULL , `nomepai` VARCHAR(40) NOT NULL , `nomemae` VARCHAR(40) NOT NULL , `nacionalidade` VARCHAR(30) NOT NULL , `naturalidade` VARCHAR(30) NOT NULL , `morada` VARCHAR(30) NOT NULL , `tipo_documento` VARCHAR(30) NOT NULL , `nr_documento` VARCHAR(20) NOT NULL , `validade_documento` VARCHAR(10) NOT NULL , `curso` VARCHAR(10) NOT NULL , `ano_frequencia` VARCHAR(10) NOT NULL , `contato_pessoal` VARCHAR(15) NOT NULL , `contato_emergencia` VARCHAR(15) NOT NULL , `estado_estudante` VARCHAR(10) NOT NULL , `data_adicionado` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP , PRIMARY KEY (`id_estudante`)) ENGINE = InnoDB; 








        ALTER TABLE `table_estudante` CHANGE `fullname` `fullname` VARCHAR(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `datanascimento` `datanascimento` VARCHAR(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `nomepai` `nomepai` VARCHAR(40) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `nomemae` `nomemae` VARCHAR(40) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `nacionalidade` `nacionalidade` VARCHAR(30) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `naturalidade` `naturalidade` VARCHAR(30) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `morada` `morada` VARCHAR(30) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `tipo_documento` `tipo_documento` VARCHAR(30) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `nr_documento` `nr_documento` VARCHAR(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `validade_documento` `validade_documento` VARCHAR(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `curso` `curso` VARCHAR(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `ano_frequencia` `ano_frequencia` VARCHAR(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `contato_pessoal` `contato_pessoal` VARCHAR(15) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL, CHANGE `contato_emergencia` `contato_emergencia` VARCHAR(15) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL; 